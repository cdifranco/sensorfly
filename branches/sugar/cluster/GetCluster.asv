function cluster = GetCluster(centers, sig, base_number)

dist = zeros(1, size(centers,1));
for i = 1:size(centers,1)
    center_sig = [];
    for j = 1 : base_number
        center_sig = [center_sig centers(i,j+3)];
    dist(i) = sum((sig - center_sig).^2).^.5;
end

[minDist cluster] = min(dist);

clear minDist;